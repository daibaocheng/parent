<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    <dubbo:application name="pmp-ims-provider" />
    <dubbo:registry id="zk1" address="zookeeper://192.168.0.100:2181" />
    <dubbo:registry id="zk2" address="zookeeper://192.168.0.101:2181" />
    <dubbo:protocol name="dubbo" port="20887" />
    <dubbo:service interface="com.vankeytech.pmp.ims.service.DemoService" ref="demoService" />
</beans>