<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    <dubbo:application name="${dubbo.appname}" />
    <dubbo:registry id="zk1" address="${dubbo.registry[0]}" />
    <dubbo:registry id="zk2" address="${dubbo.registry[1]}" />
    <dubbo:protocol name="${dubbo.protocol.name}" port="${dubbo.protocol.port}" />
    <dubbo:service interface="com.vankeytech.pmp.hms.intf.SelectUserByConditionIntf" ref="userIntfImpl" />
    <dubbo:service interface="com.vankeytech.pmp.hms.intf.BuildingIntf" ref="buildingIntfImpl" />
</beans>